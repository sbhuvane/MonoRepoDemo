            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/subapp1/tsconfig.app.json",
              "projects/subapp1/tsconfig.spec.json",
              "projects/subapp1/e2e/tsconfig.json"
            ],
            "exclude": [
