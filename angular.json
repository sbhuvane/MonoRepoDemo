            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/subapp1/e2e/protractor.conf.js",
            "devServerTarget": "subapp1:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "subapp1:serve:production"
            }
          }
        }
      }
    }},
  "defaultProject": "mainapp"
}
